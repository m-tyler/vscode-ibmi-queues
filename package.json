{
  "name":"vscode-ibmi-queues",
  "displayName":"vscode-ibmi-queues",
  "description":"IBM i Queue Objects Viewer",
  "version": "0.0.6",
  "preview":true,
  "license":"MIT",
  "engines":{"vscode":"^1.90.0"},
  "author":{"name":"Matt Tyler","url":"https://github.com/m-tyler/"},
  "publisher":"m-tyler",
  "repository":{"url":"https://github.com/m-tyler/vscode-ibmi-queues"},
  "categories":["Other"],
  "activationEvents":["onStartupFinished","onFileSystem:SplfFs","onFileSystem:MsgQFs","onFileSystem:UsrJobFs"],
  "extensionDependencies":["HalcyonTechLtd.code-for-ibmi"],
  "main":"./dist/extension.js",
  "l10n":"./l10n",
  "contributes":{
    "configuration":[
      {
        "title":"Code for IBM i Queues - Spooled Files",
        "properties":{
          "vscode-ibmi-queues.splfBrowser.openWithLineSpacing":{"type":"boolean","markdownDescription":"When checked, the default open will be with line spacing option."},
          "vscode-ibmi-queues.splfBrowser.namePattern":{
            "type":"string",
            "markdownDescription":"Additional Spooled File name mapping pattern for when Spooled Files are opened or downloaded.\n\nValid values to choose from; \n- `name` \n- `number` \n- `status` \n- `creationTimestamp` \n- `userData` \n- `size` \n- `totalPages` \n- `pageLength` \n- `qualifiedJobName` \n- `jobName` \n- `jobUser` \n- `jobNumber` \n- `formType` \n- `queueLibrary` \n- `queue` \n\nSeparate selections with commas (,). You must reload the Spooled File list for these changes to take affect.",
            "default":"name, jobName, jobUser, jobNumber, number"
          },
          "vscode-ibmi-queues.splfBrowser.tempCleanup":{"type":"boolean","markdownDescription":"When checked and VS Code closes, clean up left over temp Spooled Files from users %TMP% folder."},
          "vscode-ibmi-queues.splfBrowser.enabled":{"type":"boolean","default":true,"description":"Enable or disable Spooled File Browser."},
          "vscode-ibmi-queues.splfBrowser.filters":{"type":"array","markdownDescription":"Array of USER or OUTQ names to load spooled files from"}
        }
      },
      {
        "title":"Code for IBM i Queues - Message Queue Configuration",
        "properties":{ 
          "vscode-ibmi-queues.msgqBrowser.enabled":{"type":"boolean","default":true,"description":"Enable or disable Message Queue Browser."},
          "vscode-ibmi-queues.msgqBrowser.filters":{"type":"array","markdownDescription":"Array of USER or OUTQ names to load messages from"}
        }
      },
      {
        "title":"Code for IBM i Queues - User Jobs Configuration",
        "properties":{ 
          "vscode-ibmi-queues.userJobBrowser.enabled":{"type":"boolean","default":true,"description":"Enable or disable User Jobs Browser."},
          "vscode-ibmi-queues.userJobBrowser.filters":{"type":"array","markdownDescription":"Array of USER or OUTQ names to load jobs from"}
        }
      }
    ],
    "viewsContainers":{
      "activitybar":[ {"id":"new-SpooledFile-search","title":"Spooled Files","icon":"$(files)"},{"id":"MessageQueue-search","title":"Message Queue Messages","icon":"$(email)","when":"config.vscode-ibmi-queues.msgqBrowser"} ]
    },
    "views":{
      "ibmi-explorer":[
        {"id":"new-splfBrowser","name":"-NEW- Spooled File Browser", "when":"code-for-ibmi:connected && vscode-ibmi-queues.new-splfBrowser:splfBrowserDisabled !== true ", "visibility":"collapsed","icon":""},
        {"id":"msgqBrowser",    "name":"-NEW- Message Queue Browser","when":"code-for-ibmi:connected && vscode-ibmi-queues.msgqBrowser:msgqBrowserDisabled !== true","visibility":"collapsed","icon":""},
        {"id":"userJobBrowser", "name":"-NEW- User Jobs Browser",    "when":"code-for-ibmi:connected && vscode-ibmi-queues.userJobBrowser:userJobBrowserDisabled !== true","visibility":"collapsed","icon":""}
      ],
      "new-SpooledFile-search":[ {"id":"SpooledFileSearchView-2","name":"SPLF-Search_Results","when":"vscode-ibmi-queues.new-SpooledFile-search:searchViewVisible","icon":""} ],
      "MessageQueue-search":[ {"id":"MsgqSearchView","name":"MSGQ-Search_Results","when":"vscode-ibmi-queues.MsgqSearchView:searchViewVisible && config.vscode-ibmi-queues.msgqBrowser","icon":""} ]
    },
    "commands":[
      {"command":"vscode-ibmi-queues.splfbrowser2.filter","enablement":"code-for-ibmi:connected","title":"Filter Spooled Files","category":"IBM i SPLF","icon":"$(filter)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.search","enablement":"code-for-ibmi:connected","title":"Search Spooled Files","category":"IBM i SPLF","icon":"$(search-fuzzy)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.addUserFilter","title":"Add User Spooled File Filter","category":"IBM i SPLF","enablement":"code-for-ibmi:connected == true","icon":"$(person-add)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.addOUTQFilter","title":"Add OUTQ Spooled File Filter","category":"IBM i SPLF","enablement":"code-for-ibmi:connected == true","icon":"$(add)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.removeFilter","title":"Remove Spooled File Filter","category":"IBM i SPLF","enablement":"code-for-ibmi:connected == true","icon":"$(trash)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.deleteAll","title":"Delete All Spooled Files","category":"IBM i SPLF","enablement":"code-for-ibmi:connected == true","icon":"$(trash)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.sortFilter","title":"Sort Filters","category":"IBM i SPLF","enablement":"code-for-ibmi:connected == true","icon":"$(list-ordered)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.delete","title":"Delete SELECTED Spooled File","enablement":"code-for-ibmi:connected","icon":"$(trash)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.deleteNamed","title":"Delete like NAMED Spooled Files","enablement":"code-for-ibmi:connected","icon":"$(trash)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.removeFiltered","title":"Delete like FILTERED Spooled Files","enablement":"code-for-ibmi:connected","icon":"$(trash)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.move","title":"Move to New Output Queue","enablement":"code-for-ibmi:connected","icon":"$(move)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.downloadDefault","title":"without Line Spacing","enablement":"code-for-ibmi:connected","icon":"$(down)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.downloadWithLineSpacing","title":"with Line Spacing","enablement":"code-for-ibmi:connected","icon":"$(down)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.refreshBrowser","enablement":"code-for-ibmi:connected","title":"Refresh Spooled File Browser","category":"IBM i SPLF","icon":"$(refresh)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.sortFilesByName","title":"Name","category":"IBM i SPLF"},
      {"command":"vscode-ibmi-queues.splfbrowser2.sortFilesByDate","title":"Date","category":"IBM i SPLF"},
      {"command":"vscode-ibmi-queues.splfbrowser2.openWithDefault","enablement":"code-for-ibmi:connected","title":"with Defaults","category":"IBM i SPLF"},
      {"command":"vscode-ibmi-queues.splfbrowser2.openWithLineSpacing","enablement":"code-for-ibmi:connected","title":"with Line Spacing","category":"IBM i SPLF"},
      {"command":"vscode-ibmi-queues.splfbrowser2.openasPDF","enablement":"code-for-ibmi:connected","title":"as PDF","category":"IBM i SPLF"},
      {"command":"vscode-ibmi-queues.splfbrowser2.openWithoutLineSpacing","enablement":"code-for-ibmi:connected","title":"without Line Spacing","category":"IBM i SPLF"},
      {"command":"vscode-ibmi-queues.splfbrowser2.collapseSearchView","enablement":"code-for-ibmi:connected","title":"Collapse All","category":"IBM i SPLF","icon":"$(collapse-all)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.expandSearchView","enablement":"code-for-ibmi:connected","title":"Expand All","category":"IBM i SPLF","icon":"$(expand-all)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.closeSearchView","enablement":"code-for-ibmi:connected","title":"Close","category":"IBM i SPLF","icon":"$(close)"},
      {"command":"vscode-ibmi-queues.splfbrowser2.revealBrowser","enablement":"code-for-ibmi:connected","title":"Reveal in Spooled File Browser","category":"IBM i SPLF"},
      {"command":"vscode-ibmi-queues.splfbrowser2.createUpdatedSPOOLED_FILE_DATA_TF","enablement":"code-for-ibmi:connected","title":"Create updated version of SPOOLED_FILE_DATA ","category":"IBM i SPLF"},
      {"command":"vscode-ibmi-queues.splfbrowser2.dropUpdatedSPOOLED_FILE_DATA_TF","enablement":"code-for-ibmi:connected","title":"drop updated version of SPOOLED_FILE_DATA ","category":"IBM i SPLF"},
      {"command":"vscode-ibmi-queues.messageBrowser.filter","enablement":"code-for-ibmi:connected","title":"Filter Messages","category":"IBM i FS","icon":"$(list-filter)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.filterInquiry","enablement":"code-for-ibmi:connected","title":"Toggle to/from INQUIRY Messages","category":"IBM i FS","icon":"$(question)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.search","enablement":"code-for-ibmi:connected","title":"Search Messages","category":"IBM i Messages","icon":"$(search)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.searchBrowser","title":"Search Messages","category":"IBM i","icon":"$(search-fuzzy)","enablement":"code-for-ibmi:connected","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.addFilter","title":"Add Message Queue Filter","category":"IBM i Messages","enablement":"code-for-ibmi:connected == true","icon":"$(add)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.addUserFilter","title":"Add User Message Filter","category":"IBM i Messages","enablement":"code-for-ibmi:connected == true","icon":"$(person-add)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.removeFilter","title":"Remove Message Queue Filter","category":"IBM i Messages","enablement":"code-for-ibmi:connected == true","icon":"$(trash)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.sortFilter","title":"Sort Filters","category":"IBM i Messages","enablement":"code-for-ibmi:connected == true","icon":"$(list-ordered)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.ReplyToUnanswered","title":"Answer Message","category":"IBM i Messages","enablement":"code-for-ibmi:connected == true","icon":"$(reply)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.deleteAll","title":"Delete All Messages","category":"IBM i Messages","enablement":"code-for-ibmi:connected == true","icon":"$(clear-all)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.deleteAllButUnanswered","title":"Delete All Messages Except Unanswered","category":"IBM i Messages","enablement":"code-for-ibmi:connected == true","icon":"$(combine)", "when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.delete","title":"Delete SELECTED Message","enablement":"code-for-ibmi:connected","category":"IBM i Messages","icon":"$(delete)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.deleteByID","title":"Delete by Message ID","enablement":"code-for-ibmi:connected","category":"IBM i Messages","icon":"$(delete)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.deleteFiltered","title":"Delete all FILTERED messages","enablement":"code-for-ibmi:connected","category":"IBM i Messages","icon":"$(delete)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.refreshBrowser","title":"Refresh Message Queue Browser","enablement":"code-for-ibmi:connected","category":"IBM i Messages","icon":"$(refresh)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.refresh","title":"Refresh Message Queue","enablement":"code-for-ibmi:connected","category":"IBM i Messages","icon":"$(refresh)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.sortByID","title":"Name","enablement":"code-for-ibmi:connected","category":"IBM i Messages","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.sortByDate","title":"Date","enablement":"code-for-ibmi:connected","category":"IBM i Messages","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.collapseSearchView","title":"Collapse All Search Results","enablement":"code-for-ibmi:connected","category":"IBM i Messages","icon":"$(collapse-all)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.expandSearchView","enablement":"code-for-ibmi:connected","title":"Expand All Search Results","category":"IBM i Messages","icon":"$(expand-all)","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.closeSearchView","enablement":"code-for-ibmi:connected","title":"Close Search View","category":"IBM i Messages","icon":"$(close)"},
      {"command":"vscode-ibmi-queues.messageBrowser.revealBrowser","enablement":"code-for-ibmi:connected","title":"Reveal in User MSGQ Browser","category":"IBM i Messages","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.viewMessageDetails","enablement":"code-for-ibmi:connected","title":"View Message Details","category":"IBM i Messages","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.sendMessage","enablement":"code-for-ibmi:connected","title":"Send Message","category":"IBM i Messages","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.messageBrowser.sendInquiryMessage","enablement":"code-for-ibmi:connected","title":"Send Inquiry Message","category":"IBM i Messages","when":"config.vscode-ibmi-queues.msgqBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.sortByID","title":"Sort Jobs by Name ","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(sort-precedence)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.sortByDate","title":"Sort Jobs by Entry Date","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(sort-precedence)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.refreshBrowser","title":"Refresh User Jobs List","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(refresh)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.refresh","title":"Refresh User Jobs","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(refresh)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.revealBrowser","title":"Reveal User Job Browser","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(open-preview)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.addFilter","title":"Add User Job List Filter","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(person-add)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.sort","title":"Sort User Job Filter","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(list-ordered)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.removeFilter","title":"Remove User Job Filter","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(trash)", "when":"config.vscode-ibmi-queues.userJobBrowser.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.filterMSGW","title":"Filter Jobs by MSGW status","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(question)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.filter","title":"Filter Jobs by Value","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(filter)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.viewDetails","title":"View User Job Details","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(report)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.viewJoblog","title":"View Job Log","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected","icon":"$(report)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.ReplyToJobMessage","title":"Answer Job's Message","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected == true","icon":"$(reply)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.HoldJob","title":"Hold Job","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected == true","icon":"$(pause)","when":"config.vscode-ibmi-queues.userJobBrowser"},
      {"command":"vscode-ibmi-queues.userJobBrowser.release","title":"Release Job","category":"IBM i User Jobs","enablement":"code-for-ibmi:connected == true","icon":"$(play)","when":"config.vscode-ibmi-queues.userJobBrowser"}
    ],
    "submenus":[
      {"id":"vscode-ibmi-queues.splfbrowser2.sortSpooledFiles","label":"Sort Spooled Files by-2"},
      {"id":"vscode-ibmi-queues.splfbrowser2.openSpooledFile", "label":"Open Spooled File-2"    },
      {"id":"vscode-ibmi-queues.splfbrowser2.download",        "label":"Download Spooled File-2"},
      {"id":"vscode-ibmi-queues.messageBrowser.sort",          "label":"Sort List by-2"         },
      {"id":"vscode-ibmi-queues.messageBrowser.viewMessage",   "label":"View-2"                 },
      {"id":"vscode-ibmi-queues.userJobBrowser.sort",          "label":"Sort list by-2"         },
      {"id":"vscode-ibmi-queues.userJobBrowser.view",          "label":"View-2"                 }
    ],
    "menus":{
      "view/title":[
        {"command":"vscode-ibmi-queues.splfbrowser2.closeSearchView",     "when":"view == SpooledFileSearchView-2","group":"navigation@2"},
        {"command":"vscode-ibmi-queues.splfbrowser2.collapseSearchView",  "when":"view == SpooledFileSearchView-2","group":"navigation@1"},
        {"command":"vscode-ibmi-queues.splfbrowser2.refreshBrowser",      "when":"view == new-splfBrowser",        "group":"navigation@1"},
        {"command":"vscode-ibmi-queues.splfbrowser2.addOUTQFilter",       "when":"view == new-splfBrowser",        "group":"navigation@1"},
        {"command":"vscode-ibmi-queues.splfbrowser2.addUserFilter",       "when":"view == new-splfBrowser",        "group":"navigation@1"},
        {"command":"vscode-ibmi-queues.splfbrowser2.sortFilter",          "when":"view == new-splfBrowser",        "group":"navigation@1"},
        {"command":"vscode-ibmi-queues.messageBrowser.addFilter",         "when":"view == msgqBrowser",            "group":"navigation@2"},
        {"command":"vscode-ibmi-queues.messageBrowser.addUserFilter",     "when":"view == msgqBrowser",            "group":"navigation@3"},
        {"command":"vscode-ibmi-queues.messageBrowser.refreshBrowser",    "when":"view == msgqBrowser",            "group":"navigation@4"},
        {"command":"vscode-ibmi-queues.messageBrowser.sortFilter",        "when":"view == msgqBrowser",            "group":"navigation@5"},
        {"command":"vscode-ibmi-queues.messageBrowser.searchBrowser",     "when":"view == msgqBrowser",            "group":"navigation@6"},
        {"command":"vscode-ibmi-queues.messageBrowser.closeSearchView",   "when":"view == MsgqSearchView",         "group":"navigation@2"},
        {"command":"vscode-ibmi-queues.messageBrowser.collapseSearchView","when":"view == MsgqSearchView",         "group":"navigation@1"},
        {"command":"vscode-ibmi-queues.messageBrowser.expandSearchView",  "when":"view == MsgqSearchView",         "group":"navigation@1"},
        {"command":"vscode-ibmi-queues.userJobBrowser.addFilter",         "when":"view == userJobBrowser",         "group":"navigation@2"},
        {"command":"vscode-ibmi-queues.userJobBrowser.refreshBrowser",    "when":"view == userJobBrowser",         "group":"navigation@3"},
        {"command":"vscode-ibmi-queues.userJobBrowser.sort",              "when":"view == userJobBrowser",         "group":"navigation@4"}
      ],
      "vscode-ibmi-queues.splfbrowser2.openSpooledFile":[
        {"command":"vscode-ibmi-queues.splfbrowser2.openWithDefault"       },
        {"command":"vscode-ibmi-queues.splfbrowser2.openWithLineSpacing"   },
        {"command":"vscode-ibmi-queues.splfbrowser2.openWithoutLineSpacing"},
        {"command":"vscode-ibmi-queues.splfbrowser2.openasPDF"             }
      ],
      "vscode-ibmi-queues.splfbrowser2.sortSpooledFiles":[ {"command":"vscode-ibmi-queues.splfbrowser2.sortFilesByName"},{"command":"vscode-ibmi-queues.splfbrowser2.sortFilesByDate"} ],
      "vscode-ibmi-queues.splfbrowser2.download":[ {"command":"vscode-ibmi-queues.splfbrowser2.downloadDefault"},{"command":"vscode-ibmi-queues.splfbrowser2.downloadWithLineSpacing"} ],
      "vscode-ibmi-queues.sortMessageQueue":[ {"command":"vscode-ibmi-queues.messageBrowser.sortByID"},{"command":"vscode-ibmi-queues.messageBrowser.sortByDate"} ],
      "vscode-ibmi-queues.viewMessage":[ {"command":"vscode-ibmi-queues.messageBrowser.viewMessageDetails"} ],
      "vscode-ibmi-queues.sort":[ {"command":"vscode-ibmi-queues.userJobBrowser.sortByID"},{"command":"vscode-ibmi-queues.userJobBrowser.sortByDate"} ],
      "vscode-ibmi-queues.view":[ {"command":"vscode-ibmi-queues.userJobBrowser.viewDetails","group":"navigation@1"},{"command":"vscode-ibmi-queues.userJobBrowser.viewJoblog","group":"navigation@2"} ],
      "view/item/context":[
        {"command":"vscode-ibmi-queues.splfbrowser2.refreshBrowser"       ,"when":"view == new-splfBrowser && viewItem =~ /^splflist.*$/","group":"inline@1"},
        {"command":"vscode-ibmi-queues.splfbrowser2.filter"               ,"when":"view == new-splfBrowser && viewItem =~ /^splflist.*$/","group":"inline@2"},
        {"command":"vscode-ibmi-queues.splfbrowser2.removeFilter"         ,"when":"view == new-splfBrowser && viewItem =~ /^splflist.*$/","group":"inline@3"},
        {"command":"vscode-ibmi-queues.splfbrowser2.delete"               ,"when":"view == new-splfBrowser && viewItem =~ /^spooledfile2$/","group":"inline@4"},
        {"command":"vscode-ibmi-queues.splfbrowser2.filter"               ,"when":"view == new-splfBrowser && viewItem =~ /^splflist.*$/","group":"4_splfuser@1"},
        {"command":"vscode-ibmi-queues.splfbrowser2.search"               ,"when":"view == new-splfBrowser && viewItem =~ /^splflist.*$/","group":"4_splfuser@2"},
        {"command":"vscode-ibmi-queues.splfbrowser2.removeFilter"         ,"when":"view == new-splfBrowser && viewItem =~ /^splflist.*$/","group":"5_splfuser@1"},
        {"command":"vscode-ibmi-queues.splfbrowser2.deleteAll"            ,"when":"view == new-splfBrowser && viewItem =~ /^splflist$/","group":"5_splfuser@2"},
        {"command":"vscode-ibmi-queues.splfbrowser2.delete"               ,"when":"view == new-splfBrowser && viewItem =~ /^spooledfile2$/ ","group":"3_spooledfile@0"},
        {"command":"vscode-ibmi-queues.splfbrowser2.deleteNamed"          ,"when":"view == new-splfBrowser && viewItem =~ /^spooledfile2$/ ","group":"3_spooledfile@1"},
        {"command":"vscode-ibmi-queues.splfbrowser2.removeFiltered"       ,"when":"view == new-splfBrowser && viewItem =~ /^spooledfile2$/ ","group":"3_spooledfile@2"},
        {"command":"vscode-ibmi-queues.splfbrowser2.move"                 ,"when":"view == new-splfBrowser && viewItem =~ /^spooledfile2$/","group":"2_spooledfile@3"},
        {"command":"vscode-ibmi-queues.splfbrowser2.search"               ,"when":"view == new-splfBrowser && viewItem =~ /^spooledfile2.*$/","group":"1_spooledfile@2"},
        {"command":"vscode-ibmi-queues.splfbrowser2.filter"               ,"when":"view == new-splfBrowser && viewItem =~ /^spooledfile2.*$/","group":"1_spooledfile@1"},
